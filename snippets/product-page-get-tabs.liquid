    {%- when '1' -%}
        <div class="tabs product-tabs js-tabs" data-type="horizontal" data-postload="tabs" data-scrolling-to-opened-tab="{{ section.settings.scrolling_to_opened_tab }}">
            <div class="tabs__head" data-js-tabs-head>
                <div class="tabs__slider" data-js-tabs-slider>
                    {%- assign active_tab = ' data-active="true"' -%}
